package main

import (
	"crypto/tls"
	"fmt"
	"io"
	"net/http"
	"net/url"
	"os"
)

func m1() {
	url_i := url.URL{}
	url_proxy, _ := url_i.Parse("http://127.0.0.1:3000")

	transport := &http.Transport{}
	transport.Proxy = http.ProxyURL(url_proxy)                        // set proxy
	transport.TLSClientConfig = &tls.Config{InsecureSkipVerify: true} //set ssl

	targetUrl := "http://16.163.217.155/download/200.jpg"
	method := "GET"

	client := &http.Client{}
	client.Transport = transport
	req, err := http.NewRequest(method, targetUrl, nil)

	if err != nil {
		fmt.Println(err)
		return
	}
	// req.Header.Add("Range", "")

	res, err := client.Do(req)
	if err != nil {
		fmt.Println(err)
		return
	}
	defer res.Body.Close()

	body, err := io.ReadAll(res.Body)
	if err != nil {
		fmt.Println(err)
		return
	}

	// write to file output.jpg
	err = os.WriteFile("output.jpg", body, 0644)
	if err != nil {
		fmt.Println(err)
		return
	}

	// fmt.Println(string(body))
}

func main() {
	m1()
}
